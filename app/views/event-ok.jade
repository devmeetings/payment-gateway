extends layout-general

block content

  .container.vertical-padding-s
    .row
      .col-md-8.col-md-offset-2.col-sm-10.col-sm-offset-1
        .vertical-space-l
        .alert.alert-info
          h4.noHeaderMargins= t('ticket.success.header')
            |  #{claim.event.title}.
            .pull-left.fa.fa-thumbs-o-up.fa-4x
          h5= t('ticket.success.info')
        hr
        h5.text-muted.noHeaderMargins= t('ticket.success.contact')
          |  <code>registration@devmeetings.org</code>
        h5.text-muted.noHeaderMargins= t('ticket.success.claimId')
          |:
          code #{claim._id}

        .vertical-space-xl
